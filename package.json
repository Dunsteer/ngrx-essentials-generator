{
	"name": "ngrx-essentials-generator",
	"displayName": "Ngrx essentials generator",
	"description": "",
	"version": "0.4.0",
	"publisher": "stefanos",
	"repository": "https://github.com/Dunsteer/ngrx-essentials-generator",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.generateBoilerplateForNgrx"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.generateBoilerplateForNgrx",
				"title": "Generate boilerplate for ngrx"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.generateBoilerplateForNgrx"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
